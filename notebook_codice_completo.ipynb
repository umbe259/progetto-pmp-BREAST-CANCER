    "# -------------------------------------------\n",
    "#  DISTRIBUZIONE DEL TARGET\n",
    "# -------------------------------------------\n",
    " \n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "\n",
    "# 1. Caricamento del Dataset\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "\n",
    "# Creiamo la colonna Target con le lettere 'B' e 'M'\n",
    "# Nota: In sklearn, 0 = Malignant, 1 = Benign.\n",
    "# Mappiamo 1 -> 'B' (Benigno) e 0 -> 'M' (Maligno)\n",
    "df['Diagnosis'] = pd.Series(data.target).map({1: 'B', 0: 'M'})\n",
    "\n",
    "# 2. Impostazioni del Grafico\n",
    "plt.figure(figsize=(8, 6))\n",
    "\n",
    "# Definiamo i colori personalizzati simili all'immagine (Blu scuro e Verde)\n",
    "colors = ['#386890', '#45a675'] \n",
    "\n",
    "# 3. Creazione del Countplot\n",
    "# order=['B', 'M'] assicura che Benigno sia a sinistra e Maligno a destra\n",
    "ax = sns.countplot(x='Diagnosis', data=df, order=['B', 'M'], palette=colors)\n",
    "\n",
    "# 4. Aggiunta delle Etichette (Numeri e Percentuali)\n",
    "total = len(df) # Numero totale di pazienti (569)\n",
    "\n",
    "for p in ax.patches:\n",
    "    height = p.get_height() # L'altezza della barra (il conteggio)\n",
    "    percentage = f'{100 * height / total:.1f}%' # Calcolo percentuale\n",
    "    \n",
    "    # Scriviamo il testo sopra la barra\n",
    "    # f'{height}\\n({percentage})' crea il testo su due righe\n",
    "    ax.annotate(f'{int(height)}\\n({percentage})', \n",
    "                (p.get_x() + p.get_width() / 2., height), \n",
    "                ha='center', va='center', \n",
    "                xytext=(0, 12), # Sposta il testo 12 punti verso l'alto\n",
    "                textcoords='offset points',\n",
    "                fontsize=10, \n",
    "                color='black')\n",
    "\n",
    "# 5. Dettagli Estetici (Titoli e Griglia)\n",
    "plt.title(\"Distribuzione della Diagnosi (Target)\", fontsize=14)\n",
    "plt.xlabel(\"Diagnosi (B = Benigno, M = Maligno)\", fontsize=12)\n",
    "plt.ylabel(\"Numero di Pazienti\", fontsize=12)\n",
    "plt.ylim(0, 400) # Alziamo il limite Y per far stare le scritte\n",
    "plt.grid(axis='y', linestyle='--', alpha=0.5) # Griglia orizzontale tratteggiata\n",
    "\n",
    "# Mostra il grafico\n",
    "plt.show()\n"
   ]
