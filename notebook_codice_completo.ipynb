    "# -------------------------------------------\n",
    "#  BOXPLOT\n",
    "# -------------------------------------------\n",
    " \n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "# 1. Caricamento Dati\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "\n",
    "# Aggiungiamo la diagnosi (Target)\n",
    "df['Diagnosis'] = data.target\n",
    "df['Diagnosis'] = df['Diagnosis'].map({0: 'Malignant', 1: 'Benign'})\n",
    "\n",
    "# 2. Selezione delle 8 features specifiche (versione \"mean\")\n",
    "selected_features = [\n",
    "    'mean radius', \n",
    "    'mean texture', \n",
    "    'mean perimeter', \n",
    "    'mean area', \n",
    "    'mean smoothness', \n",
    "    'mean compactness', \n",
    "    'mean concavity', \n",
    "    'mean symmetry'\n",
    "]\n",
    "\n",
    "# Filtriamo i dati\n",
    "data_to_plot = df[selected_features]\n",
    "\n",
    "# Rinominiamo le colonne per pulizia (Rimuoviamo \"mean \" e mettiamo Maiuscola)\n",
    "clean_names = [name.replace('mean ', '').capitalize() for name in selected_features]\n",
    "data_to_plot.columns = clean_names\n",
    "\n",
    "# 3. Standardizzazione (Essenziale per confrontare Area e Smoothness)\n",
    "scaler = StandardScaler()\n",
    "data_scaled = scaler.fit_transform(data_to_plot)\n",
    "data_scaled = pd.DataFrame(data_scaled, columns=clean_names)\n",
    "\n",
    "# Riaggiungiamo la diagnosi per il grafico\n",
    "data_scaled['Diagnosis'] = df['Diagnosis']\n",
    "\n",
    "# 4. Preparazione dati per Seaborn (Melt)\n",
    "df_melted = pd.melt(data_scaled, \n",
    "                    id_vars=\"Diagnosis\",\n",
    "                    var_name=\"Features\",\n",
    "                    value_name=\"Standardized Value\")\n",
    "\n",
    "# 5. Generazione Boxplot\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.boxplot(x=\"Features\", y=\"Standardized Value\", hue=\"Diagnosis\", \n",
    "            data=df_melted, palette={\"Benign\": \"#377eb8\", \"Malignant\": \"#ff7f00\"})\n",
    "\n",
    "plt.title(\"Boxplot Standardizzato delle 8 Features Selezionate\", fontsize=16)\n",
    "plt.xlabel(\"Features\", fontsize=12)\n",
    "plt.ylabel(\"Valore Standardizzato (Z-Score)\", fontsize=12)\n",
    "plt.xticks(rotation=45)\n",
    "plt.grid(True, linestyle='--', alpha=0.3)\n",
    "plt.tight_layout()\n",
    "\n",
    "plt.show()\n",
    "\n"
