    "# -------------------------------------------\n",
    "#  PCA SCATTER PLOT\n",
    "# -------------------------------------------\n",
    " \n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn.cluster import KMeans\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "\n",
    "# 1. Caricamento del Dataset (direttamente da Sklearn)\n",
    "# Questo garantisce che i dati siano quelli ufficiali e corretti\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "\n",
    "# 2. Selezione delle Features\n",
    "# Ho selezionato le stesse 8 features \"Mean\" che avevi nel tuo codice originale.\n",
    "# Qui uso i nomi standard di sklearn che corrispondono ai tuoi.\n",
    "selected_features = [\n",
    "    \"mean radius\", \n",
    "    \"mean texture\", \n",
    "    \"mean perimeter\", \n",
    "    \"mean area\",\n",
    "    \"mean smoothness\", \n",
    "    \"mean compactness\", \n",
    "    \"mean concavity\", \n",
    "    \"mean symmetry\"\n",
    "]\n",
    "\n",
    "X = df[selected_features]\n",
    "\n",
    "# 3. Scaling (Standardizzazione)\n",
    "# Fondamentale per la PCA e K-Means, altrimenti l'Area (valori grandi) dominerebbe tutto.\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "# 4. Applicazione PCA\n",
    "pca = PCA(n_components=2)\n",
    "X_pca = pca.fit_transform(X_scaled)\n",
    "\n",
    "# Calcolo della varianza spiegata per mostrarla negli assi\n",
    "explained_variance = pca.explained_variance_ratio_\n",
    "\n",
    "# 5. Clustering K-Means\n",
    "# Impostiamo 2 cluster sperando che separino Benigni e Maligni\n",
    "kmeans = KMeans(n_clusters=2, random_state=42, n_init=10)\n",
    "labels = kmeans.fit_predict(X_scaled)\n",
    "\n",
    "# 6. Generazione del Grafico\n",
    "plt.figure(figsize=(10, 8))\n",
    "\n",
    "# Scatter plot\n",
    "scatter = plt.scatter(X_pca[:, 0], X_pca[:, 1], \n",
    "                      c=labels,           # Colora in base al cluster trovato\n",
    "                      cmap='viridis',     # Mappa colori (giallo/viola)\n",
    "                      alpha=0.7,          # Trasparenza\n",
    "                      edgecolor='k',      # Bordo nero sottile intorno ai punti\n",
    "                      s=70)               # Dimensione dei punti\n",
    "\n",
    "# Etichette assi con % di varianza\n",
    "plt.xlabel(f\"PC1 (Varianza spiegata: {explained_variance[0]*100:.1f}%)\", fontsize=12)\n",
    "plt.ylabel(f\"PC2 (Varianza spiegata: {explained_variance[1]*100:.1f}%)\", fontsize=12)\n",
    "\n",
    "# Titolo e Griglia\n",
    "plt.title(\"Analisi PCA sulle Features 'Mean' con Clustering K-Means\", fontsize=15)\n",
    "plt.grid(True, linestyle='--', alpha=0.3)\n",
    "\n",
    "# Barra laterale per i cluster\n",
    "cbar = plt.colorbar(scatter)\n",
    "cbar.set_label('Cluster Assegnato (K-Means)', rotation=270, labelpad=15)\n",
    "\n",
    "plt.show()\n",
    "\n"
