    "## SILHOUETTE PLOT GRAFICO"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "06e35ec8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# -------------------------------------------\n",
    "#  SILHOUETTE PLOT\n",
    "# -------------------------------------------\n",
    " \n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.cluster import KMeans\n",
    "from sklearn.metrics import silhouette_samples, silhouette_score\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "import matplotlib.cm as cm\n",
    "\n",
    "# --- 1. Caricamento e Preparazione Dati ---\n",
    "# Usiamo load_breast_cancer per replicare il dataset \"wdbc.data\"\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "\n",
    "# Rinominiamo le colonne per farle corrispondere alla tua lista\n",
    "rename_map = {\n",
    "    'mean radius': 'radius_mean',\n",
    "    'mean texture': 'texture_mean',\n",
    "    'mean perimeter': 'perimeter_mean',\n",
    "    'mean area': 'area_mean',\n",
    "    'mean smoothness': 'smoothness_mean',\n",
    "    'mean compactness': 'compactness_mean',\n",
    "    'mean concavity': 'concavity_mean',\n",
    "    'mean symmetry': 'symmetry_mean'\n",
    "}\n",
    "df.rename(columns=rename_map, inplace=True)\n",
    "\n",
    "# Selezione delle feature\n",
    "features_selected = [\n",
    "    \"radius_mean\", \"texture_mean\", \"perimeter_mean\", \"area_mean\",\n",
    "    \"smoothness_mean\", \"compactness_mean\", \"concavity_mean\", \"symmetry_mean\"\n",
    "]\n",
    "X = df[features_selected].astype(float)\n",
    "\n",
    "# --- 2. Standardizzazione & KMeans ---\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "k = 2\n",
    "kmeans = KMeans(n_clusters=k, random_state=42, n_init=10)\n",
    "labels = kmeans.fit_predict(X_scaled)\n",
    "\n",
    "# --- 3. Calcolo Silhouette ---\n",
    "avg_sil = silhouette_score(X_scaled, labels)\n",
    "sil_vals = silhouette_samples(X_scaled, labels)\n",
    "\n",
    "# --- 4. Plot ---\n",
    "fig, ax = plt.subplots(figsize=(9, 6))\n",
    "y_lower = 10\n",
    "\n",
    "for i in range(k):\n",
    "    # Aggrega le silhouette del cluster i-esimo e ordinale\n",
    "    ith_sil_vals = sil_vals[labels == i]\n",
    "    ith_sil_vals.sort()\n",
    "    \n",
    "    size_cluster_i = ith_sil_vals.shape[0]\n",
    "    y_upper = y_lower + size_cluster_i\n",
    "    \n",
    "    # Colore e riempimento\n",
    "    color = cm.viridis(float(i) / k)\n",
    "    ax.fill_betweenx(np.arange(y_lower, y_upper), 0, ith_sil_vals,\n",
    "                     facecolor=color, alpha=0.7)\n",
    "    \n",
    "    # Label del cluster\n",
    "    ax.text(-0.05, y_lower + 0.5 * size_cluster_i, f\"Cluster {i} (n={size_cluster_i})\", va='center')\n",
    "    \n",
    "    y_lower = y_upper + 10  # Spazio verticale tra i cluster\n",
    "\n",
    "# Linea della media\n",
    "ax.axvline(x=avg_sil, color=\"red\", linestyle=\"--\", label=f\"Avg silhouette = {avg_sil:.3f}\")\n",
    "\n",
    "# Cosmetica del grafico\n",
    "ax.set_xlabel(\"Silhouette coefficient values\")\n",
    "ax.set_ylabel(\"Sample index (ordered by cluster & silhouette)\")\n",
    "ax.set_title(f\"Silhouette plot for K-Means (k={k}) — Avg = {avg_sil:.3f}\")\n",
    "ax.set_xlim([-0.1, 1])\n",
    "ax.legend(loc='lower right')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
