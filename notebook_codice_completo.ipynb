    "# -------------------------------------------\n",
    "#  ALGORITMO K-MEANS\n",
    "# -------------------------------------------\n",
    " \n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.cluster import KMeans\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn.metrics import silhouette_score\n",
    " \n",
    "# -------------------------------------------\n",
    "# 1) Caricamento dataset\n",
    "# -------------------------------------------\n",
    "df = pd.read_csv(\n",
    "    \"https://archive.ics.uci.edu/ml/machine-learning-databases/breast-cancer-wisconsin/wdbc.data\",\n",
    "    header=None\n",
    ")\n",
    " \n",
    "# Nomi delle colonne in base alle features selezionate (sto considerando solo l'insieme delle 10 mean features)\n",
    "columns = [\"ID\", \"Diagnosis\"] + [\n",
    "    \"radius_mean\", \"texture_mean\", \"perimeter_mean\", \"area_mean\",\n",
    "    \"smoothness_mean\", \"compactness_mean\", \"concavity_mean\", \"concave_points_mean\",\n",
    "    \"symmetry_mean\", \"fractal_dimension_mean\"\n",
    "] + [f\"feature_{i}\" for i in range(20)]  # remaining columns\n",
    " \n",
    "df.columns = columns\n",
    " \n",
    "# -------------------------------------------\n",
    "# 2) selezione di 8 features principali (dall'inisieme delle 10 mean features)\n",
    "# -------------------------------------------\n",
    "features_selected = [\n",
    "    \"radius_mean\", \"texture_mean\", \"perimeter_mean\", \"area_mean\",\n",
    "    \"smoothness_mean\", \"compactness_mean\", \"concavity_mean\", \"symmetry_mean\"\n",
    "]\n",
    " \n",
    "X = df[features_selected]\n",
    " \n",
    "# -------------------------------------------\n",
    "# 3) Standardizzazione dei dati \n",
    "# -------------------------------------------\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    " \n",
    "# -------------------------------------------\n",
    "# 4) PCA per visualizzazione 2D\n",
    "# -------------------------------------------\n",
    "pca = PCA(n_components=2)\n",
    "X_pca = pca.fit_transform(X_scaled)\n",
    " \n",
    "# -------------------------------------------\n",
    "# 5)  clustering K-Means (k = 2)\n",
    "# -------------------------------------------\n",
    "kmeans = KMeans(n_clusters=2, random_state=42)\n",
    "labels = kmeans.fit_predict(X_scaled)\n",
    " \n",
    "# -------------------------------------------\n",
    "# 6) Valutazione qualità del clustering \n",
    "# -------------------------------------------\n",
    "silhouette = silhouette_score(X_scaled, labels)\n",
    "print(\"Silhouette Score:\", silhouette)\n",
    " \n",
    "# -------------------------------------------\n",
    "# 7)  PCA scatter plot \n",
    "# -------------------------------------------\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.scatter(X_pca[:, 0], X_pca[:, 1], c=labels, cmap=\"viridis\", alpha=0.7)\n",
    "plt.title(\"K-Means clustering visualized in PCA space\")\n",
    "plt.xlabel(\"PC1\")\n",
    "plt.ylabel(\"PC2\")\n",
    "plt.colorbar(label=\"Cluster\")\n",
    "plt.show()\n",
    " \n",
    "# -------------------------------------------\n",
    "# 8)  PCA  scatter plot con centroidi\n",
    "# # -------------------------------------------\n",
    "centroids = kmeans.cluster_centers_\n",
    "centroids_pca = pca.transform(centroids)\n",
    " \n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.scatter(X_pca[:, 0], X_pca[:, 1], c=labels, cmap=\"viridis\", alpha=0.5)\n",
    "plt.scatter(centroids_pca[:, 0], centroids_pca[:, 1],\n",
    "            color=\"red\", marker=\"X\", s=250, label=\"Centroidi\")\n",
    "plt.title(\"Centroidi di K-Means nello spazio PCA\")\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n"
   ]
