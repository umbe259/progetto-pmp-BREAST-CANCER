    "# -------------------------------------------\n",
    "#  ELBOW PLOT\n",
    "# -------------------------------------------\n",
    " \n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.cluster import KMeans\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "\n",
    "# --- 1. Preparazione dei Dati ---\n",
    "\n",
    "# Caricamento del Dataset (Universale tramite Sklearn)\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "\n",
    "# Selezione delle 8 Features \"Mean\"\n",
    "# Uso i nomi di sklearn che corrispondono alle tue colonne\n",
    "selected_features = [\n",
    "    \"mean radius\", \"mean texture\", \"mean perimeter\", \"mean area\",\n",
    "    \"mean smoothness\", \"mean compactness\", \"mean concavity\", \"mean symmetry\"\n",
    "]\n",
    "X = df[selected_features]\n",
    "\n",
    "# Scaling (Standardizzazione)\n",
    "# Fondamentale per K-Means perché è basato sulle distanze\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "# --- 2. Calcolo dell'Elbow Method ---\n",
    "\n",
    "inertia_values = []\n",
    "# Testiamo da 1 a 10 cluster\n",
    "ks = range(1, 11)\n",
    "\n",
    "for k in ks:\n",
    "    # Inizializziamo e addestriamo K-Means con k cluster\n",
    "    km = KMeans(n_clusters=k, random_state=42, n_init=10)\n",
    "    km.fit(X_scaled)\n",
    "    # Salviamo l'inerzia (la somma dei quadrati delle distanze intra-cluster)\n",
    "    inertia_values.append(km.inertia_)\n",
    "\n",
    "# --- 3. Generazione del Grafico ---\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "\n",
    "# Plot della linea con marcatori\n",
    "plt.plot(ks, inertia_values, \n",
    "         marker='o',        # Cerchi sui punti\n",
    "         linestyle='-',     # Linea solida\n",
    "         color='b',         # Colore blu\n",
    "         linewidth=2, \n",
    "         markersize=8)\n",
    "\n",
    "# Etichette e Titoli\n",
    "plt.xlabel(\"Numero di cluster (k)\", fontsize=12)\n",
    "plt.ylabel(\"Inertia (Within-Cluster Sum of Squares)\", fontsize=12)\n",
    "plt.title(\"Elbow Method per la selezione ottimale di k\", fontsize=15)\n",
    "\n",
    "# Miglioramenti visivi\n",
    "plt.grid(True, linestyle='--', alpha=0.3)\n",
    "plt.xticks(ks) # Mostra tutti i numeri da 1 a 10 sull'asse X\n",
    "\n",
    "# (Opzionale) Aggiungi una freccia per indicare il gomito visivo\n",
    "# Il gomito è evidente a k=2\n",
    "plt.annotate('Possibile \"Gomito\" (k=2)', \n",
