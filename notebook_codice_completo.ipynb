    "# codice random forest standard\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.metrics import (\n",
    "    classification_report,\n",
    "    confusion_matrix,\n",
    "    accuracy_score,\n",
    "    precision_score,\n",
    "    recall_score,\n",
    "    f1_score\n",
    ")\n",
    "import pandas as pd\n",
    "\n",
    "# 1) Caricamento dataset\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "df[\"label\"] = data.target   # 0 = malignant, 1 = benign\n",
    "\n",
    "# 2) Selezione delle 8 feature\n",
    "features = [\n",
    "    \"mean radius\",\n",
    "    \"mean texture\",\n",
    "    \"mean perimeter\",\n",
    "    \"mean area\",\n",
    "    \"mean smoothness\",\n",
    "    \"mean compactness\",\n",
    "    \"mean concavity\",\n",
    "    \"mean symmetry\"\n",
    "]\n",
    "\n",
    "X = df[features]\n",
    "y = df[\"label\"]\n",
    "\n",
    "# 3) Train-test split\n",
    "X_train, X_test, y_train, y_test = train_test_split(\n",
    "    X, y, test_size=0.2, stratify=y, random_state=42\n",
    ")\n",
    "\n",
    "# 4) Random Forest STANDARD\n",
    "rf_standard = RandomForestClassifier(\n",
    "    n_estimators=150,\n",
    "    random_state=42,\n",
    "  \n",
    ")\n",
    "\n",
    "# Addestramento\n",
    "rf_standard.fit(X_train, y_train)\n",
    "\n",
    "# 5) Valutazione\n",
    "y_pred = rf_standard.predict(X_test)\n",
    "\n",
    "print(\"\\n=== CLASSIFICATION REPORT (STANDARD) ===\")\n",
    "print(classification_report(y_test, y_pred, target_names=[\"malignant\", \"benign\"]))\n",
    "\n",
    "print(\"\\n=== CONFUSION MATRIX (STANDARD) ===\")\n",
    "print(confusion_matrix(y_test, y_pred))\n",
    "\n",
    "#  Metriche principali\n",
    "print(\"\\n=== METRICHE PRINCIPALI ===\")\n",
    "print(\"Accuracy :\", accuracy_score(y_test, y_pred))\n",
    "\n"
