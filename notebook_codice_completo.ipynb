    "# -------------------------------------------\n",
    "#  PCA SCATTER PLOT CON CENTROIDI\n",
    "# -------------------------------------------\n",
    " \n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn.cluster import KMeans\n",
    "from sklearn.datasets import load_breast_cancer\n",
    "\n",
    "# 1. Caricamento Dataset (Universale tramite Sklearn)\n",
    "data = load_breast_cancer()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "\n",
    "# 2. Selezione delle 8 Features \"Mean\"\n",
    "# Uso i nomi di sklearn che corrispondono alle tue colonne\n",
    "selected_features = [\n",
    "    \"mean radius\", \"mean texture\", \"mean perimeter\", \"mean area\",\n",
    "    \"mean smoothness\", \"mean compactness\", \"mean concavity\", \"mean symmetry\"\n",
    "]\n",
    "X = df[selected_features]\n",
    "\n",
    "# 3. Scaling (Standardizzazione)\n",
    "# Fondamentale: porta tutte le feature sulla stessa scala (media=0, var=1)\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "# 4. PCA (Riduzione dimensionale)\n",
    "pca = PCA(n_components=2)\n",
    "X_pca = pca.fit_transform(X_scaled)\n",
    "var_ratio = pca.explained_variance_ratio_ # Percentuale di varianza spiegata\n",
    "\n",
    "# 5. K-Means (Clustering)\n",
    "kmeans = KMeans(n_clusters=2, random_state=42, n_init=10)\n",
    "labels = kmeans.fit_predict(X_scaled)\n",
    "\n",
    "# --- IL PASSAGGIO CHIAVE PER I CENTROIDI ---\n",
